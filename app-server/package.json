{
  "name": "scanservjs-server",
  "version": "3.0.5",
  "description": "scanservjs-api is a REST based API to control your scanner.",
  "scripts": {
    "build": "node build.js",
    "clean": "npm run clean:dist && npm run clean:modules",
    "clean:dist": "rm -rf ./dist",
    "clean:modules": "rm -rf ./node_modules",
    "dev": "nodemon --inspect=9229 -e js,json,yml src/server.js",
    "lint": "eslint ./src/**/*.js ./config/config.default.js ./test/**/*.js build.js",
    "serve": "node ./src/server.js",
    "test": "mocha"
  },
  "main": "server/server.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/sbs20/scanservjs"
  },
  "devDependencies": {
    "mocha": "~11.7.5",
    "nodemon": "~3.1.11"
  },
  "author": "Sam Strachan",
  "license": "GPL-2.0",
  "dependencies": {
    "adm-zip": "0.5.16",
    "dayjs": "1.11.19",
    "express": "5.1.0",
    "express-basic-auth": "1.2.1",
    "loglevel": "1.9.2",
    "loglevel-plugin-prefix": "0.8.4",
    "mv": "2.1.1",
    "semver": "7.7.3",
    "swagger-jsdoc": "6.2.8",
    "swagger-ui-express": "5.0.1"
  },
  "nodemonConfig": {
    "watch": [
      "server"
    ]
  }
}
